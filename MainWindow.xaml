<Window x:Class="WpfTouchFrameSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" WindowState="Maximized"
        FontFamily ="./Fonts/#Press Start 2P">

    <Window.Resources>
        <FontFamily x:Key="test">/Fonts/#8BIT WONDER</FontFamily>
    </Window.Resources>

    <Grid x:Name="grid" TouchDown="OnTouchDown" TouchUp="OnTouchUp" IsManipulationEnabled="False" Width="59.77265033600158cm" Height="33.6221158140009cm" Background="#F57C00">
        <Canvas x:Name="canvas" Background="Transparent" Panel.ZIndex="1000" />
        <TextBox x:Name="xaml_touchcode_label" FontSize="30" Margin="10,5,1743.124,1201.757" Background="Transparent"  BorderThickness="0" >>>Touchcode: </TextBox>
        <TextBox x:Name="xaml_touchcode_value" FontSize="30" Margin="389,5,1246.124,1201.757" Background="Transparent" BorderThickness="0">[None]</TextBox>
        <TextBox x:Name="xaml_touchpoints" FontSize="10" Height="30" Background="Transparent" BorderThickness="0" Margin="10,39,1061.124,1201.757"></TextBox>
    </Grid>
</Window>